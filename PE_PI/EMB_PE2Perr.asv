function [Perr,stdPerr] = EMB_PE2Perr(meanPE,stdPE)
%EMB_PE2PERR Summary of this function goes here
%   Detailed explanation goes here
[P,X,T]=size(meanPE);

Perr=zeros(P,X,T);
stdPerr=zeros(
d=0.001;
D=0:d:1;
for p=1:P
    for x=1:X
        for t=1:T
            Perr(p,x,t)=1-sum(normpdf(D,0.5,meanPE(p,x,t)))*d;
        end
    end
end

end
